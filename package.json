{
  "name": "goodnight",
  "version": "0.0.0",
  "description": "Easy tool to set your computer asleep",
  "main": "electron.js",
  "scripts": {
    "dev": "webpack-dev-server",
    "electron": "NODE_ENV=development electron electron.js",
    "package": "webpack && electron-packager . --out builds --icon=./assets/goodnight.icns --overwrite",
    "webpack": "webpack -p",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marejke/goodnight.git"
  },
  "author": "Marejke Schulz <marejkeschulz@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marejke/goodnight/issues"
  },
  "homepage": "https://github.com/marejke/goodnight#readme",
  "build": {
    "appId": "goodnight.oss",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "buildResources": "assets",
      "output": "builds"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^1.7.9",
    "electron-builder": "^19.45.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "execa": "^0.8.0",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.2.1"
  }
}
